{
  "name": "filex",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "lodash": "^4.17.21",
    "pinia": "^2.3.0"
  },
  "scripts": {
    "dev": "bun --watch package/file-x-backend/index.ts",
    "seed:folders": "bun package/file-x-backend/prisma/seed/seed.ts",
    "seed:files": "bun package/file-x-backend/prisma/seed/files.ts",
    "seed:all": "bun run seed:folders && bun run seed:files",
    "prepare": "husky install",
    "commitlint": "bunx --bun commitlint --edit",
    "test": "jest"
  },
  "prisma": {
    "seed": "bun run seed:all"
  }
}